{
  "name": "fe-ent-cli",
  "version": "3.0.30",
  "description": "Ent Vue Scripts",
  "main": "dist/index.js",
  "type": "module",
  "bin": {
    "ent-cli": "/dist/cli.js"
  },
  "files": [
    "dist"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ent-framework/fe-ent-core.git"
  },
  "scripts": {
    "clean": "rimraf dist",
    "dev": "tsc --watch",
    "stub": "pnpm run build",
    "build": "pnpm run clean && pnpm run build:lib && pnpm run build:types",
    "build:lib": "tsc && node copy-template.js",
    "build:types": "tsc --emitDeclarationOnly --outDir dist",
    "lint-staged": "npx lint-staged",
    "test": "vitest",
    "lint": "eslint --max-warnings 0  \"src/**/*.{vue,ts,tsx}\" --fix",
    "prettier": "prettier --write  \"src/**/*.{tsx,ts,css,less,vue}\""
  },
  "lint-staged": {
    "*.{js,ts,jsx,tsx}": [
      "eslint --fix",
      "prettier --write"
    ]
  },
  "devDependencies": {
    "@types/clean-css": "^4.2.5",
    "@types/fs-extra": "^9.0.6",
    "@types/glob": "^8.1.0",
    "@types/inquirer": "^7.3.3",
    "@types/jest": "^26.0.24",
    "@types/jsdom": "^16.2.11",
    "@types/less": "^3.0.2",
    "@types/nunjucks": "^3.1.5",
    "@types/js-yaml": "^4.0.2",
    "@types/marked": "^5.0.0",
    "@types/prismjs": "^1.16.6",
    "@types/parse5": "^5.0.0",
    "typescript": "5.7.2",
    "jest": "^29.7.0",
    "ts-jest": "^29.1.2",
    "vue": "^3.5.13",
    "rimraf": "^6.0.1"
  },
  "dependencies": {
    "@rollup/plugin-terser": "^0.4.4",
    "@vitejs/plugin-vue": "5.2.1",
    "@vitejs/plugin-vue-jsx": "4.1.1",
    "axios": "^1.5.1",
    "chalk": "^4.1.2",
    "clean-css": "^5.2.1",
    "commander": "^7.2.0",
    "consola": "^3.3.1",
    "fast-glob": "^3.3.2",
    "fs-extra": "^11.2.0",
    "glob": "^8.1.0",
    "inquirer": "^8.1.2",
    "js-yaml": "^4.1.0",
    "jsdom": "^24.0.0",
    "less": "^4.1.1",
    "marked": "^5.1.0",
    "mockjs": "^1.1.0",
    "moment": "^2.29.1",
    "nunjucks": "^3.2.3",
    "ora": "^5.2.0",
    "pkg-types": "^1.0.2",
    "prismjs": "^1.24.1",
    "rollup-plugin-visualizer": "^5.9.3",
    "rollup": "^4.43.0",
    "svgo": "^3.1.0",
    "ts-morph": "^24.0.0",
    "unocss": "^0.57.7",
    "vite": "^7.1.0",
    "vite-plugin-compression": "^0.5.1",
    "vite-plugin-html": "^3.2.0",
    "vite-plugin-inspect": "^0.7.42",
    "vite-plugin-mkcert": "^1.17.1",
    "vite-plugin-mock": "^2.9.6",
    "vite-plugin-vue-devtools": "^7.0.20",
    "vue-docgen-api": "4.56.4",
    "vite-plugin-dts": "4.3.0"
  }
}
